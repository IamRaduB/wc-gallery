{
  "env": {
    "browser": true,
    "es2021": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base",
    "eslint-config-prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0,
    "no-console": 0,
    "no-underscore-dangle": 0
  }
}
